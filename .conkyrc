background        no
out_to_console 	  yes
out_to_x	  no
update_interval	  5
total_run_times	  0
short_units	  yes
pad_percents	  3
no_buffers yes

override_utf8_locale yes

TEXT
,[
  {"full_text": "CPU", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "$cpu%","color": "\#99D7FF","separator":false,"separator_block_width":6},
  {"full_text": "${acpitemp}°C", "color": "\#d3bb0f","separator":false,"separator_block_width":6},
  {"full_text": "${freq 1} MHz", "color": "\#9bf74c","separator":false,"separator_block_width":6},
  {"full_text": "MEM", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "$mem/$memmax","color":"\#F3D194","separator":false,"separator_block_width":6},
  {"full_text": "NET", "color":"\#888888","separator":false,"separator_block_width":6},
${if_existing /proc/net/route br0}
  {"full_text": "${addr br0}","color":"\#FFFE6A","separator":false,"separator_block_width":6},
  {"full_text": "[${downspeed br0}/${upspeed br0}]","color":"\#ffffff","separator":false,"separator_block_width":6},
$endif
${if_existing /proc/net/route enp0s25}
  {"full_text": "${addr enp0s25}","color":"\#FFFE6A","separator":false,"separator_block_width":6},
  {"full_text": "[${downspeed enp0s25}/${upspeed enp0s25}]","color":"\#ffffff","separator":false,"separator_block_width":6},
$endif
${if_existing /proc/net/route wlp2s0}
  {"full_text": "wlp2s0", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${wireless_essid wlp2s0} (${addr wlp2s0}) ${wireless_link_qual_perc wlp2s0}% ","color":"\#FFFE6A","separator":false,"separator_block_width":6},
$endif
  {"full_text": "EXT","color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${execi 30 /home/dflies/.i3/external_ip.sh}","color":"\#ffffff","separator":false,"separator_block_width":6},

${if_running sshd}
  {"full_text": "SSHD","color": "\#90FF90","separator":false,"separator_block_width":6},
$endif

  {"full_text": "BAT","color": "\#888888","separator":false,"separator_block_width":6},
  {"full_text": " ${battery} / ${battery_time}","separator":false,"separator_block_width":6},

  {"full_text": "LOAD", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${loadavg 1}","color": "\#ffffff","separator":false,"separator_block_width":6},
  {"full_text": "WEATHER", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${execi 300 /home/dflies/.i3/weather.sh}","color":"\#ffffff","separator":false,"separator_block_width":6},
  {"full_text": "DATE", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${time %a %d.%m. (%b)}", "name":"date","separator":false,"separator_block_width":6},
  {"full_text": "TIME", "color":"\#888888","separator":false,"separator_block_width":6},
  {"full_text": "${time %H:%M}","separator":false,"separator_block_width":6}
]
